{
  "version": "0.1.0",
  "name": "elmish-electron",
  "displayName": "Elmish Electron",
  "private": true,
  "main": "app/main.js",
  "scripts": {
    "start": "cd src/Main && dotnet fable webpack --port free -- -w --config webpack.config.js",
    "build": "cd src/Main && dotnet fable webpack --port free -- -p --config webpack.config.js",
    "launch": "electron .",
    "release": "electron-packager . --out release --overwrite"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "electron": "^3.0.4",
    "electron-packager": "^12.2.0",
    "fable-loader": "^2.0.0",
    "loglevel": "^1.5.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
